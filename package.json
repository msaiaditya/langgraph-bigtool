{
  "name": "langgraph-bigtool",
  "version": "0.0.2",
  "description": "Build LangGraph agents with large numbers of tools",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./dist/graph": {
      "types": "./dist/graph.d.ts",
      "import": "./dist/graph.js",
      "require": "./dist/graph.js",
      "default": "./dist/graph.js"
    },
    "./graph": {
      "types": "./dist/graph.d.ts",
      "import": "./dist/graph.js",
      "require": "./dist/graph.js",
      "default": "./dist/graph.js"
    },
    "./dist/*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "import": "./dist/*.js",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    },
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run lint && npm run test && npm run build"
  },
  "dependencies": {
    "@langchain/community": "^0.3.41",
    "@langchain/core": "^0.3.46",
    "@langchain/langgraph": "^0.2.72",
    "langchain": "^0.3.23",
    "uuid": "^10.0.0",
    "zod": "^3.23.0"
  },
  "optionalDependencies": {
    "hnswlib-node": "^3.0.0"
  },
  "peerDependencies": {
    "@langchain/core": ">=0.3.0 <0.4.0",
    "@langchain/langgraph": ">=0.2.0 <0.4.0"
  },
  "devDependencies": {
    "@langchain/core": "^0.3.46",
    "@langchain/langgraph": "0.2.72",
    "@langchain/openai": "^0.3.0",
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "langchain",
    "langgraph",
    "ai",
    "llm",
    "tools",
    "agents"
  ],
  "author": "msaiaditya@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/langchain-ai/langgraph-bigtool-ts"
  }
}
